{erl_opts, [no_debug_info]}.
{deps, [
        {mstore, {git, "https://github.com/dalmatinerdb/mstore.git", {branch, "refactor-mfile"}}},
        {bitmap, {pkg, ebitmap}}
       ]}.

{escript_incl_apps,
 [mstore_inspector]}.
{escript_main_app, mstore_inspector}.
{escript_name, mstore_inspector}.
